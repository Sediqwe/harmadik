<h1>NEW ARTICLES</h1>

<%= form_with scope: :article , url: articles_path, local: true  do |f| %>
<div class="container">
<p>
<%= f.label :title %>

<%= f.text_field :title %>
</p>
<p>
<%= f.label :description %><p>

<%= f.text_area :description %>
</p>
</p>
<p>
<%= f.submit %>
</p>
<% end %>
</div>


